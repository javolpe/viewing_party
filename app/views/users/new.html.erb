<%= form_with model: @user, local: true do |f| %>
  <%= f.label :email, 'Email:' %>
  <%= f.text_field :email %>
  <%= f.label :password, 'Password:' %>
  <%= f.password_field :password %>
  <%= f.label :password_confirm, 'Confirm Password:' %>
  <%= f.password_field :password_confirm %>
  <%= f.submit 'Create User' %>
<% end %>